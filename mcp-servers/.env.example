# MCP Servers Environment Variables
# Copy this file to .env in each server directory and fill in your values

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GCP_PROJECT_NUMBER=your-project-number
GCP_LOCATION=us-central1

# Google API Key (for mcp-tokenstats)
GOOGLE_API_KEY=your-gemini-api-key

# Service Account Credentials
# Set this to the path of your service account JSON file
GOOGLE_APPLICATION_CREDENTIALS=path/to/your-service-account.json

# Pricing Configuration (optional, for mcp-reasoning-cost)
LLM_INPUT_TOKEN_PRICE_PER_M=1.25
LLM_OUTPUT_TOKEN_PRICE_PER_M=10.00